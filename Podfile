#Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'ePed' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ePed
    pod 'Firebase/Core'
    pod 'Firebase/Auth'

    pod 'Bolts'
    pod 'FBSDKCoreKit'
    pod 'FBSDKShareKit'
    pod 'FBSDKLoginKit'

    pod 'GoogleSignIn'
end
